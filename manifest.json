{
  "name": "Content Blocker",
  "description": "Light weight content blocker",
  "author": "Unz Software",
  "version": "1.3.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "all_frames": true,
      "js": ["script.js"]
    }
  ],

  "icons": { 
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png" 
  },

  "host_permissions": [
      "*://*/*"
    ]
}